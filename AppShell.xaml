<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Proiect_Aplicatia_Mobila.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proiect_Aplicatia_Mobila"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
 Title="Welcome"
 ContentTemplate="{DataTemplate local:MainPage}"
 />
        <ShellContent
 Title="About"
 ContentTemplate="{DataTemplate local:AboutPage}"
 />
        <ShellContent
 Title="My Shopping Lists"
 ContentTemplate="{DataTemplate local:ListEntryPage}"
 />
        <ShellContent Title="Categories" ContentTemplate="{DataTemplate local:Categories}" />
    </TabBar>

</Shell>
